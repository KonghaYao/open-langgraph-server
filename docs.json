{
    "$schema": "./schema.json",
    "theme": {
        "name": "opener",
        "logo": {
            "light": "/logo.svg",
            "dark": "/logo.svg",
            "page": "docs/getting-started/overview",
            "href": "https://open-langgraph-server.agent-aura.top"
        },
        "favicon": "/logo.svg",
        "icons": {
            "library": [
                {
                    "name": "lucide",
                    "default": true
                }
            ]
        },
        "appearance": {},
        "head": [],
        "fonts": {
            "body": {
                "family": "Roboto"
            }
        }
    },
    "integrations": {
        "editLink": {
            "baseUrl": "https://github.com/KonghaYao/open-langgraph-server/edit/master",
            "title": "Edit page",
            "icon": "docs:github"
        },

        "search": {
            "orama": true
        },
        ".apps": {
            "githubStar": {
                "title": "Star",
                "label": "Show your support! Star us on GitHub ⭐️",
                "href": "https://github.com/KonghaYao/open-langgraph-server",
                "dataIcon": "octicon-star",
                "dataSize": "large",
                "dataShowCount": true,
                "ariaLabel": "Star pure-graph on GitHub"
            }
        }
    },
    "seo": {
        "domain": "https://open-langgraph-server.agent-aura.top"
    },
    "navigation": {
        "anchors": {
            "header": [
                {
                    "title": "Examples",
                    "href": "https://github.com/KonghaYao/open-langgraph-server"
                },
                {
                    "title": "Feedback",
                    "href": "https://github.com/KonghaYao/open-langgraph-server/discussions",
                    "button": "primary"
                },
                {
                    "title": "Github",
                    "href": "https://github.com/KonghaYao/open-langgraph-server",
                    "icon": "docs:github"
                }
            ],
            "sidebar": {
                "bottom": [
                    {
                        "href": "https://github.com/KonghaYao/open-langgraph-server",
                        "icon": "docs:github",
                        "title": "GitHub"
                    }
                ]
            }
        },
        "tabs": [
            {
                "title": "Guide",
                "page": "docs/getting-started",
                "icon": "book"
            },
            {
                "title": "Frameworks",
                "page": "docs/frameworks/fetch",
                "icon": "puzzle"
            },
            {
                "title": "Storage",
                "page": "docs/storage/overview",
                "icon": "database"
            },
            {
                "title": "Auth",
                "page": "docs/auth/overview",
                "icon": "shield"
            },
            {
                "title": "API",
                "page": "docs/api/overview",
                "icon": "brackets"
            }
        ],
        "sidebar": [
            {
                "route": "docs",
                "pages": [
                    {
                        "group": "Introduction",
                        "pages": ["docs/getting-started/overview", "docs/getting-started/architecture"]
                    },
                    {
                        "group": "Setup",
                        "pages": [
                            "docs/getting-started/install",
                            "docs/getting-started/project-structure",
                            "docs/getting-started/studio"
                        ]
                    },
                    {
                        "group": "Migration",
                        "pages": ["docs/migration-2.0"]
                    },

                    {
                        "group": "Frameworks",
                        "pages": ["docs/frameworks/fetch", "docs/frameworks/hono", "docs/frameworks/nextjs"]
                    },
                    {
                        "group": "Frontend SDK",
                        "pages": [
                            "docs/frontend/overview",
                            "docs/frontend/provider",
                            "docs/frontend/use-chat",
                            "docs/frontend/react",
                            "docs/frontend/vue",
                            "docs/frontend/solid",
                            "docs/frontend/store"
                        ]
                    },
                    {
                        "group": "Database",
                        "pages": [
                            "docs/storage/overview",
                            "docs/storage/memory",
                            "docs/storage/sqlite",
                            "docs/storage/postgres",
                            "docs/storage/redis"
                        ]
                    },
                    {
                        "group": "Authentication",
                        "pages": ["docs/auth/overview", "docs/auth/better-auth", "docs/auth/custom-auth"]
                    },

                    {
                        "group": "Advanced Guides",
                        "pages": ["docs/advanced/overview", "docs/advanced/vite-build"]
                    },
                    {
                        "group": "Examples",
                        "pages": ["docs/examples/nextjs-chat"]
                    },

                    {
                        "group": "API Reference",
                        "pages": ["docs/api/overview"]
                    }
                ]
            }
        ]
    },
    "redirects": [
        {
            "source": "index",
            "destination": "docs/getting-started/overview"
        }
    ]
}
